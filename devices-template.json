{
    "_comment": "Copy this file to devices.json and replace placeholders with your actual device information. Do not commit devices.json to version control.",
    "example-router-password": {
        "ip": "192.168.1.1",
        "port": 22,
        "username": "admin",
        "auth": {
            "type": "password",
            "password": "your-password-here"
        }
    },
    "example-router-ssh-key": {
        "ip": "10.0.0.1",
        "port": 22,
        "username": "admin",
        "auth": {
            "type": "ssh_key",
            "private_key_path": "/path/to/your/private/key.pem"
        }
    },
    "example-router-with-ssh-config": {
        "ip": "router.example.com",
        "port": 22,
        "username": "admin",
        "ssh_config": "~/.ssh/config",
        "auth": {
            "type": "ssh_key",
            "private_key_path": "/path/to/your/private/key.pem"
        }
    },
    "example-eve-ng-device": {
        "ip": "eve-ng.example.com",
        "port": 32769,
        "username": "root",
        "ssh_config": "~/.ssh/config_jumphost",
        "auth": {
            "type": "password",
            "password": "your-password-here"
        }
    }
}
